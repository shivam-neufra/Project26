{
    "version": 2,
    "builds": [
      {
        "src": "src/index.html",
        "use": "@vercel/static"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Custom-Header",
            "value": "my-custom-header-value"
          }
        ]
      }
    ]
  }
  